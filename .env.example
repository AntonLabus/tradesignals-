# Copy this file to .env.local and fill in your API keys
ALPHA_VANTAGE_API_KEY=YOUR_ALPHA_VANTAGE_API_KEY
NEWSAPI_KEY=YOUR_NEWSAPI_KEY

# Force perfect RSI symmetry (Sell threshold = 100 - Buy threshold)
# Set to true to mirror Buy/Sell signal conditions exactly around RSI 50
RSI_STRICT_MIRROR=true

# Optional: explicitly set RSI thresholds (used when RSI_STRICT_MIRROR=false)
# RSI_BUY=55
# RSI_SELL=45
